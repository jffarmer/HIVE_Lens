<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="300dp"
        android:layout_height="10dp"
        android:gravity="center_horizontal"
        android:text="" /> 
    <TextView
        android:id="@+id/textView2"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="1. Turn *RGB*/Change Back." />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="2. Close." /> 
    <TextView
        android:id="@+id/textView4"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="3. Exit Application." /> 
    <TextView
        android:id="@+id/textView5"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="4. I hate you!" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="5. I'm an idiot." /> 
    <TextView
        android:id="@+id/textView7"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="6. What is my name?" /> 
    <TextView
        android:id="@+id/textView8"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="7. That's not my name!" />
    <TextView
        android:id="@+id/textView9"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="8. Tell me about MODIS." />
    <TextView
        android:id="@+id/textView10"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="9. What are you?" />
    <TextView
        android:id="@+id/textView11"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="10. What is your name?" />
    <TextView
        android:id="@+id/textView12"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="11. Who are you?" />
    <TextView
        android:id="@+id/textView13"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="12. What is today?" />
    <TextView
        android:id="@+id/textView14"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="13. What time is it?" />
     <TextView
        android:id="@+id/textView15"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="14. 'Number' *Operator* 'Number'" /> 
     <TextView
        android:id="@+id/textView17"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="15. Ask me about a 'DEVELOP' 'video'" /> 
     <TextView
        android:id="@+id/textView18"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:gravity="center_horizontal"
        android:text="16. HELP" /> 
    <TextView
        android:id="@+id/textView19"
        android:layout_width="300dp"
        android:layout_height="10dp"
        android:gravity="center_horizontal"
        android:text="" /> 
        
    <Button android:layout_width="300dp" android:layout_height="50dp"  android:background="@drawable/selector_camera_button"
        android:text="OK" android:textColor="@color/White" android:onClick="dismissListener"/>

</LinearLayout>